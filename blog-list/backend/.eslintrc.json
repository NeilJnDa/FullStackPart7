{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "react"
    ],
    
    "rules": {
        "linebreak-style": ["error", "windows"],
        "indent": ["error", 4],
        "no-console": "off",
        "eqeqeq" : "error",
        "no-param-reassign": 0,
        "no-underscore-dangle": 0
    }
}
